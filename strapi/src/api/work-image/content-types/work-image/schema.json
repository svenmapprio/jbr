{
  "kind": "collectionType",
  "collectionName": "work_images",
  "info": {
    "singularName": "work-image",
    "pluralName": "work-images",
    "displayName": "Work Image",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "textPosition": {
      "type": "enumeration",
      "enum": [
        "top",
        "bottom",
        "left",
        "right"
      ],
      "required": true,
      "default": "bottom"
    },
    "work": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::work.work",
      "inversedBy": "work_images"
    }
  }
}
